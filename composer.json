{
    "name": "weareplanet/sdk",
    "version": "5.0.0",
    "description": "WeArePlanet SDK for PHP",
    "keywords": [
        "weareplanet",
        "payment",
        "php",
        "sdk",
        "api"
    ],
    "homepage": "https://github.com/weareplanet/php-sdk",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Planet Merchant Services Ltd.",
            "email": "support@datatrans.ch",
            "homepage": "https://www.weareplanet.com/"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "firebase/php-jwt": "^6.8",
        "guzzlehttp/guzzle": "^7.7",
        "guzzlehttp/psr7": "^2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "friendsofphp/php-cs-fixer": "^3.20"
    },
    "autoload": {
        "psr-4": { "WeArePlanet\\Sdk\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "WeArePlanet\\Sdk\\Test\\" : "test/" }
    }
}
